{
  "name": "jarvis-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{vue,ts}": "eslint ./src --fix"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
