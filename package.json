    "vue": "^3.2.37",
    "vue-i18n": "9",
    "vue-router": "4.1.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
